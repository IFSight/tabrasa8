<?xml version="1.0" encoding="UTF-8"?>
<phpunit bootstrap="./tests/bootstrap.php"
         colors="true">
<testsuites>
    <testsuite>
        <directory>tests</directory>
        <exclude>vendor</exclude>
    </testsuite>
</testsuites>
<filter>
    <whitelist>
        <directory suffix=".php">src</directory>
        <exclude>
            <directory suffix="Interface.php">src/</directory>
        </exclude>
    </whitelist>
</filter>
</phpunit>
