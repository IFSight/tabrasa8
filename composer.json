{
    "name": "if-sight/client-project",
    "description": "Client Project Website",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/admin_toolbar": "^2.0",
        "drupal/adminimal_theme": "^1.4",
        "drupal/advagg": "^3.0",
        "drupal/allowed_formats": "^1.1",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/block_field": "^1.0@alpha",
        "drupal/captcha": "^1.0@beta",
        "drupal/cloudflare": "^1.0@alpha",
        "drupal/components": "^1.0",
        "drupal/config_split": "^1.4",
        "drupal/console": "^1.0",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/ctools": "^3.0",
        "drupal/date_popup": "^1.0",
        "drupal/easy_breadcrumb": "^1.8",
        "drupal/elasticsearch_connector": "^6.0@alpha",
        "drupal/embed": "~1.0",
        "drupal/entity": "^1.0@beta",
        "drupal/entity_browser": "^2.0@alpha",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/entity_reference_revisions": "^1.0",
        "drupal/environment_indicator": "^3.6",
        "drupal/field_group": "^3.0",
        "drupal/file_entity": "^2.0@beta",
        "drupal/fulcrum_whitelist": "^1.1",
        "drupal/geolocation": "^1.0",
        "drupal/google_analytics": "^2.1",
        "ifsight/if_helper":"^0.1",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/libraries": "^3.0@alpha",
        "drupal/linkit": "^5.0",
        "drupal/mailsystem": "^4.1",
        "drupal/masquerade": "^2.0@beta",
        "drupal/menu_block": "^1.4",
        "drupal/metatag": "^1.4",
        "ifsight/paladin": "^1.1",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/plupload": "^1.0@beta",
        "drupal/purge": "^3.0@beta",
        "drupal/purge_queuer_url": "^1.0@beta",
        "drupal/recaptcha": "^2.2",
        "drupal/redirect": "^1.0@alpha",
        "drupal/redis": "^1.0",
        "drupal/scheduler": "^1.0",
        "drupal/search_api": "^1.12",
        "drupal/simple_sitemap": "^3.0",
        "drupal/smtp": "^1.0@beta",
        "drupal/social_media_links": "^2.6",
        "drupal/token": "^1.0",
        "drupal/twig_tweak": "^2.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/varnish_purge": "^1.0",
        "drupal/viewsreference": "^1.2",
        "drupal/webform": "^5.0",
        "drush/drush": "^10.0",
        "moxiecode/plupload": "^3.1",
        "oomphinc/composer-installers-extender": "^1.1",
        "wikimedia/composer-merge-plugin": "~1.4"
    },
    "require-dev": {
        "behat/mink-selenium2-driver": "1.4.x-dev as 1.3.x-dev",
        "drupal/core-dev": "^8.8.0",
        "drupal/drupal-extension": "^4.0@beta",
        "genesis/behat-fail-aid": "^2.1",
        "guzzlehttp/guzzle": "^6.0@dev",
        "mglaman/drupal-check": "^1.1",
        "phpunit/phpunit": ">=4.8.24",
        "roave/security-advisories": "dev-master",
        "vanare/behat-cucumber-json-formatter": "~1.1.1",
        "drupal/devel": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://if-satis.s3.us-east-2.amazonaws.com/"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            }
        },
        "installer-types": [
            "component",
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
            "docroot/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
        }
    }
}
