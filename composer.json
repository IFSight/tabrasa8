{
    "name": "ifsight/drupal-project",
    "description": "IF Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://if-satis.s3.us-east-2.amazonaws.com"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "nodespark/des-connector",
                "type": "library",
                "version": "6.x-dev",
                "dist": {
                    "url": "https://github.com/nodespark/des-connector/archive/6.x.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "bower-asset/plupload": "^3.1",
        "composer/installers": "^1.7",
        "drupal/admin_toolbar": "^2.0",
        "drupal/adminimal_theme": "^1.5",
        "drupal/allowed_formats": "^1.2",
        "drupal/better_exposed_filters": "^4.0@alpha",
        "drupal/block_field": "^1.0@alpha",
        "drupal/captcha": "^1.0",
        "drupal/cloudflare": "^1.0@alpha",
        "drupal/components": "^1.1",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/ctools": "^3.2",
        "drupal/date_popup": "^1.0",
        "drupal/easy_breadcrumb": "^1.12",
        "drupal/elasticsearch_connector": "~6.0@alpha",
        "drupal/embed": "^1.3",
        "drupal/entity": "^1.0",
        "drupal/entity_browser": "^2.4",
        "drupal/entity_embed": "^1.0",
        "drupal/entity_reference_revisions": "^1.8",
        "drupal/environment_indicator": "^3.7",
        "drupal/field_group": "^3.0",
        "drupal/fulcrum_whitelist": "^1.1",
        "drupal/geolocation": "^1.11",
        "drupal/google_analytics": "^3.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/libraries": "^3.0@alpha",
        "drupal/linkit": "^4.3",
        "drupal/mailsystem": "^4.2",
        "drupal/masquerade": "^2.0@beta",
        "drupal/menu_block": "^1.5",
        "drupal/metatag": "^1.11",
        "drupal/paragraphs": "^1.11",
        "drupal/pathauto": "^1.6",
        "drupal/plupload": "^1.0@beta",
        "drupal/purge": "^3.0@beta",
        "drupal/purge_queuer_url": "^1.0@RC",
        "drupal/recaptcha": "^2.5",
        "drupal/redirect": "^1.5",
        "drupal/redis": "^1.4",
        "drupal/scheduler": "^1.1",
        "drupal/search_api": "^1.15",
        "drupal/simple_sitemap": "^3.5",
        "drupal/smtp": "^1.0@beta",
        "drupal/social_media_links": "^2.6",
        "drupal/token": "^1.6",
        "drupal/twig_tweak": "^2.5",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/varnish_purge": "^1.12",
        "drupal/viewsreference": "^1.4",
        "ifsight/if_admin": "^0.1.0",
        "ifsight/if_helper": "^0.1.0",
        "ifsight/paladin": "^1.1",
        "ifsight/varnish_purge_tags": "^0.2.0",
        "oomphinc/composer-installers-extender": "^1.1"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8",
        "drupal/devel": "^2.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            },
            "file-mapping": {
                "[web-root]/robots.txt": false,
                "[web-root]/README.txt": false,
                "[web-root]/.ht.router.php": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/web.config": false,
                "[web-root]/.htaccess": false,
                "[web-root]/.eslintignore": false,
                "[web-root]/sites/default/default.services.yml": false,
                "[web-root]/sites/default/default.settings.php": false,
                "[web-root]/sites/development.services.yml": false,
                "[web-root]/sites/example.settings.local.php": false,
                "[web-root]/sites/example.sites.php": false
            },
            "omit-defaults": true,
            "excludes": [
                "docroot/README.txt",
                "docroot/robots.txt",
                "docroot/.ht.router.php",
                "docroot/INSTALL.txt",
                "docroot/example.gitignore"
            ]
        },
        "installer-types": [
            "component",
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/libraries/{$name}": ["type:drupal-library", "type:npm-asset", "type:bower-asset", "type:component"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
            "docroot/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
        }
    }
}
