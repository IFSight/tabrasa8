{
    "name": "drupal/drupal",
    "description": "Drupal is an open source content management platform powering millions of websites and applications.",
    "type": "project",
    "license": "GPL-2.0+",
    "require": {
        "composer/installers": "^1.0.24",
        "wikimedia/composer-merge-plugin": "~1.4",
        "cweagans/composer-patches": "^1.6",
        "drupal/core": "^8.6",
        "drupal/admin_toolbar": "^1.0",
        "drupal/advagg": "^3.0",
        "drupal/allowed_formats": "^1.1",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/block_field": "^1.0@alpha",
        "drupal/captcha": "^1.0@beta",
        "drupal/components": "^1.0",
        "drupal/ctools": "^3.0",
        "drupal/date_popup": "^1.0",
        "drupal/devel": "^1.0",
        "drupal/easy_breadcrumb": "^1.8",
        "drupal/embed": "~1.0",
        "drupal/entity": "^1.0@beta",
        "drupal/entity_browser": "^2.0@alpha",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/entity_reference_revisions": "^1.0",
        "drupal/field_group": "^1.0",
        "drupal/file_entity": "^2.0@beta",
        "drupal/geolocation": "^1.0",
        "drupal/google_analytics": "^2.1",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/libraries": "^3.0@alpha",
        "drupal/linkit": "^5.0",
        "drupal/mailsystem": "^4.1",
        "drupal/masquerade": "^2.0@beta",
        "drupal/menu_block": "^1.4",
        "drupal/metatag": "^1.4",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/plupload": "^1.0@beta",
        "drupal/purge": "^3.0@beta",
        "drupal/purge_queuer_url": "^1.0@beta",
        "drupal/recaptcha": "^2.2",
        "drupal/redirect": "^1.0@alpha",
        "drupal/redis": "^1.0",
        "drupal/scheduler": "^1.0",
        "drupal/simple_sitemap": "^2.0",
        "drupal/smtp": "^1.0@beta",
        "drupal/social_media_links": "^2.6",
        "drupal/token": "^1.0",
        "drupal/twig_tweak": "^2.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/varnish_purge": "^1.0",
        "drupal/video_embed_field": "^2.0@alpha",
        "drupal/viewsreference": "^1.2",
        "drupal/webform": "^5.0",
        "drupal/youtube": "^1.0@beta",
        "drupal/console": "~1.0",
        "moxiecode/plupload": "^3.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "autoloader-suffix": "Drupal8"
    },
    "extra": {
        "_readme": [
            "By default Drupal loads the autoloader from ./vendor/autoload.php.",
            "To change the autoloader you can edit ./autoload.php.",
            "This file specifies the packages.drupal.org repository.",
            "You can read more about this composer repository at:",
            "https://www.drupal.org/node/2718229"
        ],
        "merge-plugin": {
            "include": [
                "core/composer.json"
            ],
            "recurse": false,
            "replace": false,
            "merge-extra": false
        },
        "installer-paths": {
            "core": ["type:drupal-core"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"],
            "modules/custom/{$name}": ["type:drupal-custom-module"],
            "themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "patches" : {
            "drupal/core": {
                "Replaces 'thumbnail' text with actual alt text for thumbnail formatter": "sites/patches/core/media-IFCUSTOM-fix-thumbnail-image-alt-text.patch"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Core\\Composer\\": "core/lib/Drupal/Core/Composer"
        }
    },
    "scripts": {
        "pre-autoload-dump": "Drupal\\Core\\Composer\\Composer::preAutoloadDump",
        "post-autoload-dump": [
          "Drupal\\Core\\Composer\\Composer::ensureHtaccess"
        ],
        "post-package-install": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup",
        "post-package-update": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "moxiecode/plupload",
                "type": "drupal-library",
                "version": "3.1.2",
                "dist": {
                    "url": "https://github.com/moxiecode/plupload/archive/v3.1.2.zip",
                    "type": "zip"
             },
          "require": { "composer/installers": "~1.0" }
            }
        }
    ]
}
