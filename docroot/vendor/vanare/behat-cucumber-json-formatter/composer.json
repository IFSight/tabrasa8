{
  "name": "vanare/behat-cucumber-json-formatter",
  "description": "Behat extension for generating json reports",
  "keywords": ["behat", "json", "formatter", "cucumber", "jenkins"],
  "homepage": "https://github.com/Vanare/behat-cucumber-formatter",
  "license": "MIT",
  "authors": [
    {
      "name": "Vanare Team",
      "email": "opensource@vanare.com",
      "homepage": "http://www.vanare.com/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "behat/behat": "~3.0"
  },
  "minimum-stability": "dev",
  "config": {
    "bin-dir": "bin/"
  },
  "require-dev": {
    "mikey179/vfsStream": "1.6.0",
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "Vanare\\BehatCucumberJsonFormatter\\": "src/"
    }
  }
}
